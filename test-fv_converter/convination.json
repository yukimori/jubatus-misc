	],
	"combination_types": {},
	"combination_rules": [
	    { "key_left": "*@num", "key_right": "*@num", "type": "add"},
	    { "key_left": "*@num", "key_right": "*@num", "type": "mul"},
	    { "key_left": "*@str#bin/bin", "key_right": "*@str#bin/bin", "type": "add"},
	    { "key_left": "*@str#bin/bin", "key_right": "*@str#bin/bin", "type": "mul"}
	]
    }
}
